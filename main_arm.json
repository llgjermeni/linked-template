{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "type": "string"
        },
        "_artifactsLocationSasToken": {
            "type": "securestring"
        }
    },
    "variables": {
        "adls-gen1-Folder": "azure_adls_gen1_arm",
        "adls-gen1-FileName": "adls_gen1_arm.json",
        "adls-gen1-ParametersFileName": "adls_gen1_arm.parameters.json",
        "adls-gen2-Folder": "azure_adls_gen2_arm",
        "adls-gen2-FileName": "adls_gen2_arm.json",
        "adls-gen2-ParametersFileName": "adls_gen2_arm.parameters.json"
    },

    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "name": "AdlsGen1Template",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/', variables('adls-gen1-Folder'), '/', variables('adls-gen1-FileName'), parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/', variables('adls-gen1-Folder'), '/', variables('adls-gen1-ParametersFileName'), parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "name": "AdlsGen2Template",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/', variables('adls-gen2-Folder'), '/', variables('adls-gen2-FileName'), parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/', variables('adls-gen2-Folder'), '/', variables('adls-gen2-ParametersFileName'), parameters('_artifactsLocationSasToken'))]",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ],
    "outputs": {
    },
    "functions": [
    ]
}